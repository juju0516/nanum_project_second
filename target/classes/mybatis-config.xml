	</settings>
	<!-- vo의 별칭 만들기  -->
	<typeAliases>
		<typeAlias type="com.ict.login.model.vo.Kakao_LoginVO" alias="klvo"/>
		
		<typeAlias type="com.ict.member.model.vo.MemberVO" alias="mvo"/>
		<typeAlias type="com.ict.member.model.vo.PointVO" alias="pvo"/>
		<typeAlias type="com.ict.member.model.vo.InterestVO" alias="ivo"/>
		<typeAlias type="com.ict.member.model.vo.InquiryVO" alias="iqvo"/>
		
		<typeAlias type="com.ict.alarm.model.vo.AlarmVO" alias="almvo"/>
		
		<typeAlias type="com.ict.project.model.vo.ProjectVO" alias="prjvo"/>
		<typeAlias type="com.ict.project.model.vo.Prj_RegiVO" alias="regivo"/>
		<typeAlias type="com.ict.project.model.vo.RegularVO" alias="rvo"/>
		<typeAlias type="com.ict.project.model.vo.DonatorVO" alias="dvo"/>
		
		<typeAlias type="com.ict.special.model.vo.Special_DonateVO" alias="sdvo"/>
		
