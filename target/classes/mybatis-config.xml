		<typeAlias type="com.ict.story.model.vo.StoryVO" alias="strvo"/>
		
		<typeAlias type="com.ict.manager.model.vo.ManagerVO" alias="mngvo"/>	
		<typeAlias type="com.ict.manager.model.vo.MngrSearchVO" alias="msvo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/login_mapper.xml"/>
		<mapper resource="mappers/member_mapper.xml"/>
		<mapper resource="mappers/basket_mapper.xml"/>
		<mapper resource="mappers/goods_mapper.xml"/>
		<mapper resource="mappers/project_mapper.xml"/>
		<mapper resource="mappers/story_mapper.xml"/>
		<mapper resource="mappers/search_mapper.xml"/>
